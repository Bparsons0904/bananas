# Environment variables for Bananas development

# Ports
# Note: Using 5433 to avoid conflicts with system PostgreSQL on 5432
DB_PORT=5433
TILT_PORT=10350

# Framework ports
STANDARD_PORT=8081
GIN_PORT=8082
FIBER_PORT=8083
ECHO_PORT=8084
CHI_PORT=8085
GORILLA_PORT=8086

# Docker environment
DOCKER_ENV=dev

# Database settings
DB_HOST=localhost
DB_USER=bananas_user
DB_PASSWORD=bananas_pass
DB_NAME=bananas_dev
DB_SSL_MODE=disable